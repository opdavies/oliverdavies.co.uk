{% set events = [] %}

{% for date in site.events.dates|reverse if date.date < "today"|date("Y-m-d") %}
    {% set events = events|merge([{
        date: date,
        event: site.events.events[date.event],
        talks: data.talks,
    }]) %}
{% endfor %}

{% include 'talks-table' with { events: events } %}
