<li>
    <strong>{% spaceless %}
        {% if talk.fuzzy_date %}
            {{ talk.date|date('F') }}
        {% else %}
            {{ talk.date|date('jS F') }}
        {% endif %}
    {% endspaceless %}</strong> - {{ talk.location }}
    -
    {% if talk.fuzzy_date %}
        {{ talk.title }}
    {% else %}
        <a href="{{ talk.url }}">{{ talk.title }}</a>
    {% endif %}
</li>
