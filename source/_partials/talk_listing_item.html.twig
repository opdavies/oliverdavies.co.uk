<li>{% spaceless %}
    <strong>
        {% if not talk.fuzzy_date %}
            {{ talk.date|date('jS F') }}
        {% else %}
            {{ talk.date|date('F') }}
        {% endif %}
    </strong> - {{ talk.location }} -
    <a href="{{ talk.url }}">
        {{ talk.title }}
    </a>
{% endspaceless %}</li>
